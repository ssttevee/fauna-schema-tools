{
  "name": "fauna-schema-tools",
  "version": "0.0.8",
  "license": "MIT",
  "type": "module",
  "main": "dist/lib.cjs",
  "module": "dist/lib.mjs",
  "types": "dist/lib.d.ts",
  "bin": "dist/main.cjs",
  "scripts": {
    "prepack": "npx patch-package && npm run build",
    "build:wasm": "zig build -freference-trace -Doptimize=ReleaseSmall -Dtarget=wasm32-wasi && zbind dist/root.wasm src/zig.ts",
    "build:js": "rollup -c",
    "build": "npm run build:wasm && npm run build:js"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "anymatch": "^3.1.3",
    "chokidar": "^3.6.0",
    "cmd-ts": "^0.13.0",
    "glob-parent": "^5.1.2",
    "jennifer-js": "^1.0.3",
    "readdirp": "^3.6.0",
    "source-map": "^0.7.4",
    "zbind": "^0.0.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/glob-parent": "^5.1.3",
    "@types/node": "^20.14.11",
    "patch-package": "^8.0.0",
    "rollup": "*",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-uint8-array": "^0.1.2",
    "tslib": "^2.6.3"
  },
  "exports": {
    ".": {
      "import": "./dist/lib.mjs",
      "require": "./dist/lib.cjs",
      "types": "./dist/lib.d.ts"
    },
    "./wasm": {
      "import": "./dist/root.wasm",
      "require": "./dist/root.wasm"
    },
    "./wasm-embedded": {
      "import": "./dist/root.wasm.mjs",
      "require": "./dist/root.wasm.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "rollup": "^4.20.0"
  }
}
